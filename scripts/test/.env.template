# Virtuoso API Test Configuration Template
# Copy this file to .env and fill in your actual credentials
# DO NOT COMMIT the .env file to version control

VIRTUOSO_BASE_URL=https://api.virtuoso.example.com
VIRTUOSO_AUTH_TOKEN=your-auth-token-here
VIRTUOSO_CLIENT_ID=your-client-id-here
VIRTUOSO_CLIENT_NAME=your-client-name-here

# Optional test configuration
VIRTUOSO_TEST_TIMEOUT=30
VIRTUOSO_TEST_RETRY=3
VIRTUOSO_TEST_DEBUG=false
